{
    "name": "Giants Theme",
    "type": "dark",
    "colors": {
        "editor.background": "#27251F", //#333
        "editor.foreground": "#fff",
        "editorLineNumber.foreground": "#EFD19F",
        "editorLineNumber.activeForeground": "#FA4616",
        "editor.selectionBackground": "#FFFFFF3D",
        "selection.background": "#EFD19F",
        // Activity Bar Colors
        "activityBar.foreground": "#EFD19F",
        "activityBar.background": "#27251F",
        "activityBarBadge.background": "#FA4616",
        "activityBarBadge.foreground": "#FFFFFF",
        "activityBar.border": "#333",
        // Sidebar Colors
        "sideBar.background": "#27251F",
        "sideBar.border": "#333",
        "sideBarTitle.foreground": "#EFD19F",
        "sideBarSectionHeader.background": "#333",
        "sideBarSectionHeader.foreground": "#EFD19F",
        "activityBar.activeBorder": "#FA4616",
        "activityBar.activeBackground": "#4b473c"


    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "#7F7F7F",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "foreground": "#FA4616"
            }
        },
        {
            "name": "Tag",
            "scope": "entity.name.tag, meta.tag.sgml, markup.deleted.git_gutter, variable.other.constant",
            "settings": {
                "foreground": "#FA4616"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": "keyword.operator, punctuation, punctuation.definition.tag, punctuation.separator.inheritance.php, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.section.embedded, keyword.other.template, keyword.other.substitution",
            "settings": {
                "foreground": "#FC9074"
            }
        },
        {
            "name": "Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#AE8F6F"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": "constant.character.escape, variable.other, variable.parameter, string, constant.other.symbol, constant.other.key, entity.other.inherited-class, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
            "settings": {
                "foreground": "#EFD19F"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": "keyword, storage.type, storage.modifier",
            "settings": {
                "foreground": "#AE8F6F"
            }
        },
        {
            "name": "Function, Special Method, Block Level",
            "scope": "entity.name.function, meta.function-call, variable.function, support.function, keyword.other.special-method, meta.block-level",
            "settings": {
                "foreground": "#FC9074"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": "support.other.variable, string.other.link, entity.name.type",
            "settings": {
                "foreground": "#F2777A"
            }
        },
        {
            "name": "Support",
            "scope": "support.type",
            "settings": {
                "foreground": "#EFD19F"
            }
        },
        {
            "name": "Number, Constant, Tag Attribute, Embedded",
            "scope": "constant.numeric, constant.language, support.constant, constant.character, keyword.other.unit, constant.other.color",
            "settings": {
                "foreground": "#fff"
            }
        },
        {
            "name": "Test",
            "scope": "",
            "settings": {
                "foreground": "#ff00bf"
            }
        }
    ]
}
